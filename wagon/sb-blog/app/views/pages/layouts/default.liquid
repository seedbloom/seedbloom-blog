---
title: Default layout
is_layout: false
---
<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>{{ site.name }}</title>

    <link rel="shortcut icon" href="{{ 'favicon.png' | theme_image_url }}">

    {{ '//fonts.googleapis.com/css?family=Roboto:300,400,500,600' | stylesheet_tag }}
    {{ 'http://seedbloom:3000/css/sb-common.css' | stylesheet_tag }}

    {{ 'bootstrap.css' | stylesheet_tag }}
    {{ 'application.css' | stylesheet_tag }}
  </head>
  <body>
    {% block content %}
    {% endblock %}

    {{ 'jquery.min.js' | javascript_tag }}
    {{ 'bootstrap.min.js' | javascript_tag }}
    {{ 'application.js' | javascript_tag }}
    {{ 'http://seedbloom:3000/js/sb-common.js' | javascript_tag }}
  </body>
</html>
